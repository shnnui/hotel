<p id="notice"><%= notice %></p>

<h1>Hotels</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Ota channel</th>
      <th>Third party hotel</th>
      <th>Hotel brand</th>
      <th>Name</th>
      <th>Address</th>
      <th>Open at</th>
      <th>Close at</th>
      <th>Phone</th>
      <th>Fax</th>
      <th>Zip code</th>
      <th>Is economic</th>
      <th>Is apartmen</th>
      <th>Reocar city</th>
      <th>City name</th>
      <th>City</th>
      <th>Business zone</th>
      <th>Business zone</th>
      <th>Attached business zone</th>
      <th>District</th>
      <th>District</th>
      <th>Baidu location</th>
      <th>Google location</th>
      <th>Star</th>
      <th>Credit cards</th>
      <th>Status</th>
      <th>Review count</th>
      <th>Review good</th>
      <th>Review poor</th>
      <th>Review score</th>
      <th>Intro</th>
      <th>Description</th>
      <th>Traffic</th>
      <th>Has coupon</th>
      <th>Themes</th>
      <th>Room total</th>
      <th>Facilities</th>
      <th>Stay leave info</th>
      <th>Recommend level</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hotels.each do |hotel| %>
      <tr>
        <td><%= hotel.id %></td>
        <td><%= hotel.ota_channel %></td>
        <td><%= hotel.third_party_hotel_id %></td>
        <td><%= hotel.hotel_brand_id %></td>
        <td><%= hotel.name %></td>
        <td><%= hotel.address %></td>
        <td><%= hotel.open_at %></td>
        <td><%= hotel.close_at %></td>
        <td><%= hotel.phone %></td>
        <td><%= hotel.fax %></td>
        <td><%= hotel.zip_code %></td>
        <td><%= hotel.is_economic %></td>
        <td><%= hotel.is_apartmen %></td>
        <td><%= hotel.reocar_city_id %></td>
        <td><%= hotel.city_name %></td>
        <td><%= hotel.city_id %></td>
        <td><%= hotel.business_zone %></td>
        <td><%= hotel.business_zone_id %></td>
        <td><%= hotel.attached_business_zone %></td>
        <td><%= hotel.district %></td>
        <td><%= hotel.district_id %></td>
        <td><%= hotel.baidu_location %></td>
        <td><%= hotel.google_location %></td>
        <td><%= hotel.star %></td>
        <td><%= hotel.credit_cards %></td>
        <td><%= hotel.status %></td>
        <td><%= hotel.review_count %></td>
        <td><%= hotel.review_good %></td>
        <td><%= hotel.review_poor %></td>
        <td><%= hotel.review_score %></td>
        <td><%= hotel.intro %></td>
        <td><%= hotel.description %></td>
        <td><%= hotel.traffic %></td>
        <td><%= hotel.has_coupon %></td>
        <td><%= hotel.themes %></td>
        <td><%= hotel.room_total %></td>
        <td><%= hotel.facilities %></td>
        <td><%= hotel.stay_leave_info %></td>
        <td><%= hotel.recommend_level %></td>
        <td><%= link_to 'Show', hotel %></td>
        <td><%= link_to 'Edit', edit_hotel_path(hotel) %></td>
        <td><%= link_to 'Destroy', hotel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

